{
    "name": "shadi/exporter",
    "require": {
        "shadiakiki1986/git-rest-api-client": "*",
        "ext-yaml": "*",
        "symfony/console": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "jakub-onderka/php-parallel-lint": "^0.9.2"
    },
    "authors": [
        {
            "name": "Shadi Akiki",
            "email": "shadiakiki1986@gmail.com"
        }
    ],
    "autoload": {
      "psr-4": {
        "PdoGit\\": "src"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "PdoGit\\": "test"
      }
    },
    "bin": [
      "bin/pdo-git"
    ],
    "scripts": {
      "test": [
        "parallel-lint . --exclude vendor",
        "phpunit --stderr --verbose"
      ]
    }

}
